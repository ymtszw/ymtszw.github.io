{
  "name": "ymtszw",
  "scripts": {
    "postinstall": "elm-tooling install && patch-package",
    "fetch_recent_twilogs": "TZ=Asia/Tokyo node fetch_recent_twilogs.mjs; [ -n \"$(git status --porcelain data/)\" ] && git commit --include data/ -m \"ci: fetched recent twilogs ($(date))\"",
    "import_archived_tweets": "TZ=Asia/Tokyo node import_archived_tweets.js",
    "start": "elm-pages dev --port 10101",
    "build": "elm-pages build"
  },
  "devDependencies": {
    "csvtojson": "^2.0",
    "elm-optimize-level-2": "^0.2",
    "elm-pages": "^2.1",
    "elm-review": "^2.5",
    "elm-tooling": "^1.4",
    "node-fetch": "^3.3.1",
    "patch-package": "^6.5"
  }
}
